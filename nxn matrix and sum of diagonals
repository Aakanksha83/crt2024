nxn matrix and sum of diagonals
#include <stdio.h>

int main() {
    int n;
    scanf("%d",&n);
    int a[n][n];
    int i,j,s=0;
    for(i=0;i<n;i++){
        for(j=0;j<n;j++){
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<n;i++){
            for(j=0;j<n;j++){
                printf("%d",a[i][j]);
            }
            printf("\n");
    }
        for(i=0;i<n;i++){
            for(j=0;j<n;j++){
                if(i=j){
                s=s+a[i][j];
                
            } 
            }
        }
        printf("\n%d",s);
           
        
    
  

    return 0;
}
output:
3
1 2 3
2 3 4
4 5 6
123
234
456

9
