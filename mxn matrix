mxn matrix
include <stdio.h>

int main() {
    int m,n;
    scanf("%d%d",&m,&n);
    int a[m][n];
    int i,j;
    for(i=0;i<m;i++){
        for(j=0;j<n;j++){
            scanf("%d",&a[i][j]);
        }
    }
        for(i=0;i<m;i++){
            for(j=0;j<n;j++){
            printf("%d",a[i][j]);
        }
        printf("\n");
           
        }
    
  

    return 0;
}



output;
4
4
7 4 2 1
4 5 8 2
7 5 83 2
8 4 3 2
7421
