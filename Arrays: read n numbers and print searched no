read n numbers and print searched no
#include <stdio.h>

int main() {
    int n;
    scanf("%d",&n);
    int a[n],i;
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int sno;
    scanf("%d",&sno);
    for(i=0;i<n;i++){
        if(sno==a[i]){
            printf("element found");
            break;
            
        }
    }
    if(i==n){
        
    
    printf("not found");

}    return 0;
}

output:
4
6
6
89
8
6
element found
