sum of odd and even 

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,e=0,o=0;
    scanf("%d",&n);
    
    int a[n],i;
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++){
        
    
    if(a[i]%2==0){
        e=e+a[i];
        
    }
    else{
      o=o+a[i];
    }
}
        printf("%d\n",o);
        printf("%d",e);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
