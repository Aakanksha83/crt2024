mxn and transpose matrix
#include <stdio.h>

int main() {
    int m,n;
    scanf("%d%d",&m,&n);
    int a[m][n];
    int i,j;
    for(i=0;i<m;i++){
        for(j=0;j<n;j++){
            scanf("%d",&a[i][j]);
        }
    }
        for(i=0;i<m;i++){
            for(j=0;j<n;j++){
            printf("%d",a[j][i]);
        }
        printf("\n");
           
        }
    
  

    return 0;
}
output:
3 
3
3 4 2 
2 3 6
2 3 6
322
433
266
